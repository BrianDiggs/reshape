
test_simple <- function() {
  checkEqualsNumeric(ninteraction(list(1:5)), 1:5)
  checkEqualsNumeric(ninteraction(list(1:5, 1)), 1:5)
  checkEqualsNumeric(ninteraction(list(1:5, 1, 1)), 1:5)
  checkEqualsNumeric(ninteraction(list(1, 1:5)), 1:5)
  checkEqualsNumeric(ninteraction(list(1, 1, 1:5)), 1:5)

  checkEqualsNumeric(ninteraction(list(1:2, 1:10), drop = T), 1:10)
}